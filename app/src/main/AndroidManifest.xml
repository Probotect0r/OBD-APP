<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sagar.myapplication">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".RpmActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TemperatureActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LineChartActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Main2Activity"
            android:label="@string/title_activity_main2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecentDriveInfo"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".DrivesByDate" />
        <activity android:name=".ViewErrorCodes" />
        <activity android:name=".ThrottlePosition"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".MassAirFlow"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".FuelPressure"
            android:theme="@style/AppTheme.NoActionBar"/>
    </application>

</manifest>